        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.atmcorp.caraccountant.expenseReport"></activity>
        <activity android:name="com.atmcorp.caraccountant.incomeReport" />
        <activity android:name="com.atmcorp.caraccountant.expense" />
        <activity android:name="com.atmcorp.caraccountant.income" />
        <activity android:name="com.atmcorp.caraccountant.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>