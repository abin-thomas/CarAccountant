<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cLayoutMaster"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.atmcorp.caraccountant.income">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_datetop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_datebottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_kmstarttop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.12" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_kmstartbottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.17" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_kmendstart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".19" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_kmendbottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".24" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_incometop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".26" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_incomebottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".31" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_nohsttop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".33" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_nohstbottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".38" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hsttop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".4" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hstbottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".45" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_netincometop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".47" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_netincomebottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".52" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_btnstop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".55" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_btnsbottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".60" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_graphtop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".65" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_graphbottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vertical1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".02" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vertical2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".4" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vertical3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".42" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vertical4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".45" />

    <TextView
        android:id="@+id/lbl_Date"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text="Date"
        app:layout_constraintBottom_toTopOf="@+id/guideline_datebottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical1"
        app:layout_constraintTop_toTopOf="@+id/guideline_datetop" />

    <TextView
        android:id="@+id/lbl_startKms"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text="Starting Odometer"
        app:layout_constraintBottom_toTopOf="@+id/guideline_kmstartbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical1"
        app:layout_constraintTop_toTopOf="@+id/guideline_kmstarttop" />

    <TextView
        android:id="@+id/lbl_endKms"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text="Ending Odometer"
        app:layout_constraintBottom_toTopOf="@+id/guideline_kmendbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical1"
        app:layout_constraintTop_toTopOf="@+id/guideline_kmendstart" />

    <TextView
        android:id="@+id/lbl_totalIncome"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text="Total Income"
        app:layout_constraintBottom_toTopOf="@+id/guideline_incomebottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical1"
        app:layout_constraintTop_toTopOf="@+id/guideline_incometop" />

    <TextView
        android:id="@+id/lbl_noHstIncome"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text="Approx Income(No HST)"
        app:layout_constraintBottom_toTopOf="@+id/guideline_nohstbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical1"
        app:layout_constraintTop_toTopOf="@+id/guideline_nohsttop" />

    <TextView
        android:id="@+id/lbl_approxHST"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text="Approx HST"
        app:layout_constraintBottom_toTopOf="@+id/guideline_hstbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical1"
        app:layout_constraintTop_toTopOf="@+id/guideline_hsttop" />

    <TextView
        android:id="@+id/lbl_netIncome"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text="Net Income"
        app:layout_constraintBottom_toTopOf="@+id/guideline_netincomebottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical1"
        app:layout_constraintTop_toTopOf="@+id/guideline_netincometop" />

    <Button
        android:id="@+id/btn_submit"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Submit"
        app:layout_constraintBottom_toTopOf="@+id/guideline_btnsbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical5"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical4"
        app:layout_constraintTop_toTopOf="@+id/guideline_btnstop" />

    <Button
        android:id="@+id/btn_clear"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:onClick="clearAll"
        android:text="Clear"
        app:layout_constraintBottom_toTopOf="@+id/guideline_btnsbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical1"
        app:layout_constraintTop_toTopOf="@+id/guideline_btnstop" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vertical5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".98" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text=":"
        app:layout_constraintBottom_toTopOf="@+id/guideline_datebottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical3"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintTop_toTopOf="@+id/guideline_datetop" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text=":"
        app:layout_constraintBottom_toTopOf="@+id/guideline_kmstartbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical3"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintTop_toTopOf="@+id/guideline_kmstarttop" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text=":"
        app:layout_constraintBottom_toTopOf="@+id/guideline_kmendbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical3"
        app:layout_constraintHorizontal_bias="0.529"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintTop_toTopOf="@+id/guideline_kmendstart" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text=":"
        app:layout_constraintBottom_toTopOf="@+id/guideline_incomebottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical3"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintTop_toTopOf="@+id/guideline_incometop" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text=":"
        app:layout_constraintBottom_toTopOf="@+id/guideline_nohstbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical3"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintTop_toTopOf="@+id/guideline_nohsttop" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text=":"
        app:layout_constraintBottom_toTopOf="@+id/guideline_hstbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical3"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintTop_toTopOf="@+id/guideline_hsttop" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text=":"
        app:layout_constraintBottom_toTopOf="@+id/guideline_netincomebottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical3"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical2"
        app:layout_constraintTop_toTopOf="@+id/guideline_netincometop" />

    <EditText
        android:id="@+id/txt_date"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:focusable="false"
        android:inputType="none"
        app:layout_constraintBottom_toTopOf="@+id/guideline_datebottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical5"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical4"
        app:layout_constraintTop_toTopOf="@+id/guideline_datetop" />

    <EditText
        android:id="@+id/txt_startKms"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/guideline_kmstartbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical5"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical4"
        app:layout_constraintTop_toTopOf="@+id/guideline_kmstarttop" />

    <EditText
        android:id="@+id/txt_endKms"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/guideline_kmendbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical5"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical4"
        app:layout_constraintTop_toTopOf="@+id/guideline_kmendstart" />

    <EditText
        android:id="@+id/txt_totalIncome"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/guideline_incomebottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical5"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical4"
        app:layout_constraintTop_toTopOf="@+id/guideline_incometop" />

    <EditText
        android:id="@+id/txt_noHstIncome"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="0"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/guideline_nohstbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical5"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical4"
        app:layout_constraintTop_toTopOf="@+id/guideline_nohsttop" />

    <TextView
        android:id="@+id/txt_approxHst"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="false"
        android:gravity="center_vertical"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline_hstbottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical5"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical4"
        app:layout_constraintTop_toTopOf="@+id/guideline_hsttop" />

    <TextView
        android:id="@+id/txt_netIncome"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline_netincomebottom"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical5"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical4"
        app:layout_constraintTop_toTopOf="@+id/guideline_netincometop" />


</androidx.constraintlayout.widget.ConstraintLayout>